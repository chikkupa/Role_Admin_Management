<html>
<head>
<title>Admin Dashboard</title>
</head>
<body>
	<h1>Welcome to admin dashboard</h1>
	<table>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Gender</th>
			<th>Role</th>
			<th>Status</th>
		</tr>
    {{range .List}}
    	<tr>
    		<td>{{.ID}}</td>
    		<td>{{.Name}}</td>
    		<td>{{.LastName}}</td>
    		<td>{{.Email}}</td>
    		<td>{{.Gender}}</td>
    		<td>
    			{{if eq .Role 1 }}Admin{{end}}
    			{{if eq .Role 2 }}User Type 1{{end}}
    			{{if eq .Role 3 }}User Type 2{{end}}
    			{{if eq .Role 4 }}User Type 3{{end}}
    		</td>
    		<td>
    			{{if eq .Status 1}}Approved{{end}}
    			{{if eq .Status 2}}Not Active{{end}}
    		</td>
    	</tr>
    {{end}}
	</table>
</body>
</html>